<?xml version="1.0" encoding="utf-8"?>

<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <include latin:keyboardLayout="@xml/key_styles_common" />
    <Row latin:keyWidth="10%p">
        <include latin:keyboardLayout="@xml/rowkeys_khmer1" />
    </Row>
    <Row latin:keyWidth="10%p">
        <include latin:keyboardLayout="@xml/rowkeys_khmer2" />
    </Row>
    <Row latin:keyWidth="10%p">
        <Key latin:keyStyle="shiftKeyStyle" latin:keyWidth="15%p" />
        <include latin:keyboardLayout="@xml/rowkeys_khmer3" />
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillRight" />
    </Row>
    <include latin:keyboardLayout="@xml/row_qwerty4" />
</merge>
